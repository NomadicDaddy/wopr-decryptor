{
	"compilerOptions": {
		"declaration": true,
		"emitDeclarationOnly": false,
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"module": "ES2022",
		"moduleResolution": "Bundler",
		"outDir": "dist",
		"skipLibCheck": true,
		"strict": true,
		"target": "ES2022"
	},
	"include": ["src"]
}
